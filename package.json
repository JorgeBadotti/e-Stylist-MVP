    "express": "^4.22.1",
    "express-session": "^1.18.2",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.2",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
