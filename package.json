{
  "name": "e-stylist-mvp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install-all": "npm install && npm install --prefix client && npm install --prefix server",
    "dev": "concurrently \"npm run dev --prefix client\" \"nodemon --watch server --exec node server/app.js\"",
    "build:client": "npm run build --prefix client",
    "server": "node server/app.js",
    "start": "npm run build:client && node server/app.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.22.1",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local-mongoose": "^9.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}