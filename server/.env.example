# Server Environment Variables

PORT=3000
NODE_ENV=development

# Google Gemini API (necessário para análise de imagens)
GOOGLE_GEMINI_KEY=sua_chave_aqui

# MongoDB (banco de dados)
MONGODB_URI=mongodb://localhost:27017/estylis

# Google OAuth (para autenticação)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Backend URL (para callbacks)
BACKEND_URL=http://localhost:3000
# Frontend URL (para redirecionamento após login)
FRONTEND_URL=http://localhost:5173

# Cloudinary (para upload de imagens)
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=
CLOUDINARY_CLOUD_NAME=
